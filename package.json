  "main": "./src/electron-starter.js",
  "scripts": {
    "prestart": "node ./scripts/prestart.js",
    "start": "cp ./env/dev.env .env && nf start",
    "production": "npm run build && electron .",
    "react-start": "webpack-dev-server --config webpack.dev.js",
    "electron": "./node_modules/.bin/electron",
    "electron-start": "electron .",
    "electron-rebuild": "electron-rebuild",
    "build": "cp ./env/prod.env .env && webpack --config webpack.prod.js",
    "win-packager": "npm run build && electron-packager . Bootcamp-Manager --overwrite --platform=win32 --arch=ia32 --prune=true --out=executable --version-string.ProductName=\"Electron Tutorial App\""
